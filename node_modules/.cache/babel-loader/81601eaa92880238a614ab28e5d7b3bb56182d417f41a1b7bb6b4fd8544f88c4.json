bmit button -->\n    <button type=\"submit\" class=\"btn btn-primary text-centre btn-block\">\n      Sign in\n    </button>\n  </form>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      css: [\"form-control\"],\n      dt: {\n        email: \"\",\n        loginPass: \"\",\n      },\n    };\n  },\n  methods: {\n    async login() {\n      let con =\"\"\n      if (this.dt?.email && this.dt.loginPass) {\n        await axios\n          .post(\"http://localhost:6969/login\", this.dt)\n          .then((res) => {\n            con=res.data;\n          })\n          .catch((e) => console.log(e));\n\n          if (con.data == \"success\") alert(\"Login successful\");\n            if (con.data == \"wrong\") alert(\"Invalid password\");\n            if (con.data == \"notegistered\") alert(\"Not registered\");\n        this.css = this.css.filter((item) => item !== \"error\");\n      } else {\n        alert(\"please fill the data !\");\n        this.css.push(\"error\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.px-5 {\n  padding-right: 35rem !important;\n  padding-left: 35rem !important;\n}\n.py-5 {\n  padding-top: 8rem !important;\n  padding-bottom: 3rem !important;\n}\n.error {\n  border: 1px solid red;\n}\n</style>\n"],"mappings":";AAsCA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,CAAC,cAAc,CAAC;MACrBC,EAAE,EAAE;QACFC,KAAK,EAAE,EAAE;QACTC,SAAS,EAAE;MACb;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAIC,GAAE,GAAG,EAAC;MACV,IAAI,IAAI,CAACL,EAAE,EAAEC,KAAI,IAAK,IAAI,CAACD,EAAE,CAACE,SAAS,EAAE;QACvC,MAAML,KAAI,CACPS,IAAI,CAAC,6BAA6B,EAAE,IAAI,CAACN,EAAE,EAC3CO,IAAI,CAAEC,GAAG,IAAK;UACbH,GAAG,GAACG,GAAG,CAACV,IAAI;QACd,CAAC,EACAW,KAAK,CAAEC,CAAC,IAAKC,OAAO,CAACC,GAAG,CAACF,CAAC,CAAC,CAAC;QAE7B,IAAIL,GAAG,CAACP,IAAG,IAAK,SAAS,EAAEe,KAAK,CAAC,kBAAkB,CAAC;QAClD,IAAIR,GAAG,CAACP,IAAG,IAAK,OAAO,EAAEe,KAAK,CAAC,kBAAkB,CAAC;QAClD,IAAIR,GAAG,CAACP,IAAG,IAAK,cAAc,